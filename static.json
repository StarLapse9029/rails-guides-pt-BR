{
  "clean_urls": true,
  "https_only": true,
  "root": "site/",
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/**": {
      "Cache-Control": "no-store, no-cache, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=512000"
    },
    "/javascripts/**": {
      "Cache-Control": "public, max-age=512000"
    }
  }
}
